{
  "name": "api",
  "version": "1.0.0",
  "description": "RESTful API with Node.js, Express, and PostgreSQL for Fatboar",
  "main": "./bin/www",
  "dependencies": {
    "bcrypt": "^3.0.6",
    "core-util-is": "^1.0.2",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "helmet": "^3.20.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "pm2": "^3.5.1",
    "sequelize": "^5.19.2",
    "sequelize-cli": "^5.5.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cross-env": "^6.0.3",
    "faker": "^4.1.0",
    "mocha": "^6.2.2"
  },
  "scripts": {
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:reset": "npx sequelize-cli db:migrate:undo:all && npm run migrate",
    "pretest": "cross-env NODE_ENV=test npm run migrate:reset",
    "test": "cross-env NODE_ENV=test mocha",
    "start:dev": "./node_modules/.bin/sequelize db:migrate && nodemon -L ./bin/www",
    "start": "node ./bin/www"
  },
  "keywords": [],
  "author": "Walid SADAOUI, Charles ATANGUI, Sonia METCHUM, Mamadou MBEGUERE",
  "license": "ISC"
}
